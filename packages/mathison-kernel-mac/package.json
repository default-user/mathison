{
  "name": "mathison-kernel-mac",
  "version": "1.0.0",
  "description": "Mathison OI Kernel for macOS (Intel/Apple Silicon) with local LLM",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "mathison": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc && chmod +x dist/cli.js",
    "build:pkg": "npm run build && npm run pkg:build",
    "pkg:build": "pkg dist/cli.js -t node18-macos-x64 -o mathison",
    "test": "jest",
    "clean": "rm -rf dist mathison"
  },
  "keywords": ["mathison", "oi", "kernel", "llm", "local", "beamstore", "identity"],
  "dependencies": {
    "mathison-storage": "workspace:*",
    "better-sqlite3": "^9.2.2",
    "commander": "^11.1.0",
    "chalk": "^4.1.2",
    "node-fetch": "^2.7.0",
    "readline": "^1.3.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.8",
    "@types/node": "^18.0.0",
    "@types/node-fetch": "^2.6.9",
    "typescript": "^5.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "pkg": "^5.8.1"
  }
}
